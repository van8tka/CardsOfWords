{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "paths": {
      "baseUrl": "./",
      "@utils/*": [
        "src/utils/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@navigators/*": [
        "src/navigators/*"
      ],
      "@primitives/*": [
        "src/primitives/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@components/*": [
        "src/components/*"
      ]
    }
  }
}
